server {
      listen 80;
      listen [::]:80;
      server_name p.mawaqit.net;

      location /.well-known/acme-challenge/ {
          root /var/www/certbot;
      }

      # redirect HTTP to HTTPS
       location / {
          return 301 https://$host$request_uri;
       }
}

server {
    listen 443 ssl;
    server_name p.mawaqit.net;
    location / {
        proxy_pass http://example.org;
    }
}
